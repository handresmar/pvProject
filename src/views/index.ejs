<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <title> <%= title %></title>
    <% include partials/head %>
</head>
<body>
    <div class="wraper">
        <% include partials/nav %>
        
        <!--Cajas resumen-->
        <section class="boxes">
            <!--Caja 1-->
            <div class="power">
                <h3 class="uni">Potencia [kW]</h3>
                <hr/>
                <i class="ico fas fa-power-off fa-3x" style="color:#86DF83"></i>
                <h2 class="vl">123,12</h2>
            </div>
            <!--Caja 2-->
            <div class="energy">
                    <h3 class="uni">Energía [kWh]</h3>
                    <hr/>
                    <i class="ico fas fa-bolt fa-3x" style="color: #EEFF00"></i>
                    <h2 class="vl">123,12</h2>
            </div>
            <!--Caja 3-->
            <div class="irradiance">
                    <h3 class="uni">Irradiancia [W/m²]</h3>
                    <hr/>
                    <i class="ico fas fa-sun fa-3x" style="color: #FFD500"></i>
                    <h2 class="vl">123,12</h2>
            </div>
        </section>
        <!--graficas-->
        <section class="plots">
            <canvas id="myCanvas" width="100" height="400"></canvas>
        </section>
        <!--Footer-->
        <% include partials/footer %>
    </div>
    <!-- Wrapper Ends-->    
</body>
</html>